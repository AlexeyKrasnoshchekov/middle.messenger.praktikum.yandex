     <div class="chat">
          <aside class="chat_sidebar">
               <div class="chat_top">
                    {{{inputSearch}}}
               </div>
               <div class="chat_items_list">
                    {{{chatItem}}}
               </div>
          </aside>
          <section class="chat_place">
               <div class="chat_place_top">
                    <div class="chat_user">
                         {{{avatar}}}
                         <p class="chat_user_name">Вадим</p>
                    </div>
                    <span class="chat_menu">{{{buttonMenu}}}</span>
               </div>
               <div class="chat_place_middle"></div>
               <div class="chat_place_bottom">
                    <div class="chat_attachment">
                         {{{buttonAttach}}}
                         <div class="chat_attachment_choice">
                              <div class="chat_attachment_type"><span><i class="fa fa-file-image" aria-hidden="true"></i></span><span>Фото или Видео</span></div>
                              <div class="chat_attachment_type"><span><i class="fa fa-file" aria-hidden="true"></i></span><span>Файл</span></div>
                              <div class="chat_attachment_type"><span><i class="fa fa-location-arrow" aria-hidden="true"></i></span><span>Локация</span></div>
                         </div>
                    </div>
                    <div class="chat_message_input">
                         {{{inputMessage}}}
                    </div>
                    <div class="chat_send">
                         {{{buttonSend}}}
                    </div>
               </div>
          </section>
     </div>

